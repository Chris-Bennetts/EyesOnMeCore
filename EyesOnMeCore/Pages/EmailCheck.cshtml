    </style>
</head>
<body>
    <main>
        <h1> Subject Action Requests Sent! </h1>
    </main>
    <script>

        function postrequest() {
            const xhttp = new XMLHttpRequest();
            xhttp.onload = function () {
                document.getElementById("demo").innerHTML = this.responseText;
            }
            xhttp.open("POST", "EmailRequest");

            text = document.getElementById("subjecttext").value, useridtemp;
            const obj = JSON.stringify(text);
            xhttp.send(obj);
        }

    </script>
</body>